    @{
        string msisdn = base.Context.Session.GetString("Msisdn");
        bool isSubscribed = base.Context.Session.TryGetValue("IsSubscribed", out byte[] isSubscribedData);
        string endSubscriptionDateString = base.Context.Session.GetString("EndOfSubscription");

        DateTime.TryParse(endSubscriptionDateString, out DateTime endOfSubscriptionDate);

        if (endOfSubscriptionDate > DateTime.Now)
        {
            <iframe src="@ViewBag.GamePath" width="100%" height="100%"></iframe>
        }
        else
        {
            <p>You need to renew your subscription to play this game.</p>
            <a href="/Subscribers/Subscribe">Renew Subscription</a>
        }
    }
